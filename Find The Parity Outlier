def find_outlier(integers):
    print(integers)
    n_pari = 0
    pari = 0
    n_dispari = 0
    for i in integers:
        if i % 2 == 0 or i == 0:
            n_pari += 1
            pari = i
        else:
            n_dispari = i
    print(n_pari,n_dispari)
    if n_pari == 1:
        return pari
    return n_dispari
